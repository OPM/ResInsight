START
10 MAI 2007 /

RUNSPEC

DIMENS
  10 10 10 /

GRID

PERMX
  1000*0.25 /

COPY
  PERMX PERMY /
  PERMX PERMZ /
/


SCHEDULE

WELSPECS 
    'W_1'        'GROUP_BJARNE'   30   37  1*       'OIL'  7* /
    'W_2'        'GROUP_ODD'   31   18  1*       'GAS'  7* /
    'W_3'        'GROUP_BJARNE'   20   51  1*       'WATER'  7* /

/

WGRUPCON
   'W_1' 2* OIL /
-- the spaces around the name are intentional!
   ' W_2 ' NO /
   'W_3' YES 100 RAT 0.5 /
/

TSTEP
 10 /

END
