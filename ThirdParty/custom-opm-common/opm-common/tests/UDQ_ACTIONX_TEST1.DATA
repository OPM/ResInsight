GCONPROD
'TEST' 'LRAT' 6000 1* 1* 6000 'RATE' 'NO' 9* /
'LOWER' 'FLD' 6000 1* 1* 6000 'RATE' 'YES' 1* 'FORM' 7* /
'UPPER' 'FLD' 3000 1* 1* 6000 'RATE' 'YES' 1* 'FORM' 7* /
/

GCONINJE
'LOWER'      'WATER'   'VREP'  3*   1.2  /
'UPPER'      'WATER'   'VREP'  3*   2.0  /
/

