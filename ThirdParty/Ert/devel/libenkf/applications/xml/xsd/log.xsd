<?xml version="1.0" encoding="ISO-8859-1" ?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:include schemaLocation="basetypes.xsd"/>

    <xs:element name="log">
        <xs:complexType>
            <xs:all maxOccurs="1" minOccurs="0">
                <xs:element name="file" type="path" minOccurs="0" maxOccurs="1"/>
                <xs:element name="level" type="xs:nonNegativeInteger" minOccurs="0" maxOccurs="1"/>
                <xs:element name="update_log_path" type="path" minOccurs="0" maxOccurs="1"/>
            </xs:all>
        </xs:complexType>
    </xs:element>
</xs:schema>